#bill-diff
  margin-top 10px

  .text
    border solid 1px
    padding  8px
    display inline-block
    background #ffffff

    .add
      background #adff2f
    .remove
      background #CD5C5C
      color #ffffff

#references
  margin-top 10px

  .added,.removed
    &:before
      content:" "
      display inline-block
      margin-right: 8px;
      background #adff2f
      width 16px
      height 16px

  .removed:before
      background #CD5C5C
